<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:GUI.ViewModel"
                    xmlns:view="clr-namespace:GUI.View">
    <DataTemplate DataType="{x:Type vm:NhaVM}">
        <view:Nha />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:PhongVM}">
        <view:Phong />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:HoaDonVM}">
        <view:HoaDon />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DichVuVM}">
        <view:DichVu />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:KhachThueVM}">
        <view:KhachThue />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:BaoCaoVM}">
        <view:BaoCao />
    </DataTemplate>
</ResourceDictionary>